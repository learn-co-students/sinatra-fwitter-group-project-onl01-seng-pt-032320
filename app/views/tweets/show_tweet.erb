<h1>Fwitter</h1>

<h2><%= @tweet.content %></h2>
<h3>By: <%= @tweet.user.username %></h3>


<div style="text-align: center;">
  <a href="/tweets/<%= @tweet.id %>/edit" style="text-align: center;">Edit Tweet</a>
</div>
  <form action="/tweets/<%= @tweet.id %>/delete" method="post">
    <input type="hidden" id="hidden" name="_method" value="delete">
    <input type="submit" name="" value="Delete Tweet">
  </form>
